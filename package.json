{
  "name": "wompi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "start:dev": "nodemon ./src/server.js"
  },
  "jest": {
    "verbose": true,
    "passWithNoTests": true,
    "collectCoverageFrom": [
      "src/infrastructure/controllers/*.{js,jsx}",
      "src/infrastructure/services/*.{js,jsx}",
      "src/infrastructure/utils/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "nodemon": "^2.0.20"
  }
}